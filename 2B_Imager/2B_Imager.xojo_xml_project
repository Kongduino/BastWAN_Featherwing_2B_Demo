    <SourceLine>Sub Opening()</SourceLine>
    <SourceLine>Var imgType As New FileType</SourceLine>
    <SourceLine>imgType.Name = "images/all"</SourceLine>
    <SourceLine>imgType.Extensions = "jpg;jpeg;png;bmp"</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Me.AcceptFileDrop(imgType)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
