   </ItemSource>
  </HookInstance>
  <HookInstance>
   <ItemName>DropObject</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>1481154559</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub DropObject(obj As DragItem, action As DragItem.Types)</SourceLine>
    <SourceLine>lbLog.RemoveAllRows()</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If Not obj.FolderItemAvailable Then</SourceLine>
    <SourceLine>lbLog.AddRow "No FolderItem available!"</SourceLine>
    <SourceLine>Return</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Dim p, q, m As Picture</SourceLine>
    <SourceLine>p = Picture.Open(obj.FolderItem)</SourceLine>
    <SourceLine>If p = Nil Then</SourceLine>
    <SourceLine>lbLog.AddRow "No image available!"</SourceLine>
    <SourceLine>Return</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Self.Title = obj.FolderItem.Name</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>lbLog.AddRow "Image name: " + obj.FolderItem.Parent.Name + "/" + obj.FolderItem.Name</SourceLine>
    <SourceLine>lbLog.AddRow "Image size: " + Str(p.Width) + " : " + Str(p.Height)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>q = ResizeImage(p, me.Width, me.Height)</SourceLine>
    <SourceLine>m = New Picture(me.Width, me.Height)</SourceLine>
    <SourceLine>m.Graphics.DrawPicture(q, (me.Width - q.Width)\2, (me.Height - q.Height)\2)</SourceLine>
    <SourceLine>me.Backdrop = m</SourceLine>
    <SourceLine>me.Refresh()</SourceLine>
    <SourceLine>lbLog.AddRow "Resized image size: " + Str(q.Width) + " : " + Str(q.Height)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Dim f As FolderItem</SourceLine>
    <SourceLine>f = obj.FolderItem.Parent.Child(obj.FolderItem.Name + ".2b")</SourceLine>
    <SourceLine>If f.Exists Then f.Remove()</SourceLine>
    <SourceLine>lbLog.AddRow "Image name: " + f.Parent.Name + "/" + f.Name</SourceLine>
    <SourceLine>Dim bs As BinaryStream</SourceLine>
    <SourceLine>bs = BinaryStream.Create(f)</SourceLine>
    <SourceLine>If bs = Nil Then</SourceLine>
    <SourceLine>lbLog.AddRow "Couldn't create image for output!"</SourceLine>
    <SourceLine>Return</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>lbLog.AddRow "Saving..."</SourceLine>
    <SourceLine>Dim mb As MemoryBlock</SourceLine>
    <SourceLine>mb = New MemoryBlock(6)</SourceLine>
    <SourceLine>mb.StringValue(0, 2) = "2B"</SourceLine>
    <SourceLine>mb.UInt16Value(2) = q.Width</SourceLine>
    <SourceLine>mb.UInt16Value(4) = q.Height</SourceLine>
    <SourceLine>bs.Write(mb)</SourceLine>
    <SourceLine>Dim rs As RGBSurface</SourceLine>
    <SourceLine>rs = q.RGBSurface</SourceLine>
    <SourceLine>Dim x, y As Integer</SourceLine>
    <SourceLine>Dim cc As UInt16</SourceLine>
    <SourceLine>Dim rc, gc, bc As UInt8</SourceLine>
    <SourceLine>Dim c As Color</SourceLine>
    <SourceLine>For y = 0 To q.Height-1</SourceLine>
    <SourceLine>mb = New MemoryBlock(q.Width * 2)</SourceLine>
    <SourceLine>For x = 0 To q.Width-1</SourceLine>
    <SourceLine>c = rs.Pixel(x, y)</SourceLine>
    <SourceLine>rc = BitwiseAnd(c.Red\8, &amp;b11111)</SourceLine>
    <SourceLine>gc = BitwiseAnd(c.Green\4, &amp;b111111)</SourceLine>
    <SourceLine>bc = BitwiseAnd(c.Blue\8, &amp;b11111)</SourceLine>
    <SourceLine>cc = Bitwise.ShiftLeft(rc, 11) + Bitwise.ShiftLeft(gc, 5) + bc</SourceLine>
    <SourceLine>// cc = rc * 2048 + gc * 32 + bc</SourceLine>
    <SourceLine>mb.UInt16Value(x*2) = cc</SourceLine>
    <SourceLine>Next</SourceLine>
    <SourceLine>bs.Write(mb)</SourceLine>
    <SourceLine>Next</SourceLine>
    <SourceLine>bs.Close()</SourceLine>
    <SourceLine>lbLog.AddRow "Done!"</SourceLine>
    <SourceLine>f.Parent.Open()</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>lbLog.AddRow "Checking..."</SourceLine>
    <SourceLine>bs = BinaryStream.Open(f)</SourceLine>
    <SourceLine>If bs = Nil Then</SourceLine>
    <SourceLine>lbLog.AddRow "Couldn't reopen the file!!!"</SourceLine>
    <SourceLine>Return</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>mb = bs.Read(6)</SourceLine>
    <SourceLine>If mb.StringValue(0, 2) &lt;&gt; "2B" Then</SourceLine>
    <SourceLine>lbLog.AddRow "Wrong signature!"</SourceLine>
    <SourceLine>bs = Nil</SourceLine>
    <SourceLine>Return</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Dim w, h As Integer</SourceLine>
    <SourceLine>w = mb.UInt16Value(2)</SourceLine>
    <SourceLine>h = mb.UInt16Value(4)</SourceLine>
    <SourceLine>lbLog.AddRow "Image size: " + Str(w) + " : " + Str(h)</SourceLine>
    <SourceLine>q = New Picture(w, h)</SourceLine>
    <SourceLine>rs = q.RGBSurface</SourceLine>
    <SourceLine>For y = 0 To h - 1</SourceLine>
    <SourceLine>mb = bs.Read(w * 2)</SourceLine>
    <SourceLine>For x = 0 To w - 1</SourceLine>
    <SourceLine>cc = mb.UInt16Value(x*2)</SourceLine>
    <SourceLine>rc = BitwiseAnd(Bitwise.ShiftRight(cc, 11), &amp;b11111) * 8</SourceLine>
    <SourceLine>gc = BitwiseAnd(Bitwise.ShiftRight(cc, 5), &amp;b111111) * 4</SourceLine>
    <SourceLine>bc = BitwiseAnd(cc, &amp;b11111) * 8</SourceLine>
    <SourceLine>rs.Pixel(x, y) = RGB(rc, gc, bc)</SourceLine>
    <SourceLine>Next</SourceLine>
    <SourceLine>Next</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>m = New Picture(me.Width, me.Height)</SourceLine>
