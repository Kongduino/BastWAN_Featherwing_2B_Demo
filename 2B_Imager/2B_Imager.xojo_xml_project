   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function ResizeImage(p As Picture, maxWidth As Integer, maxHeight As Integer) As Picture</SourceLine>
   <SourceLine>Dim pw, ph, newH, newW As Integer</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>pw = p.Width</SourceLine>
   <SourceLine>ph = p.Height</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If pw &lt;= maxWidth And ph &lt;= maxHeight Then Return p</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim q As Picture</SourceLine>
   <SourceLine>Dim r0, r1 As Double</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>r0 = pw / maxWidth</SourceLine>
   <SourceLine>r1 = ph / maxHeight</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If r1 &gt; r0 Then r0 = r1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>newW = pw / r0</SourceLine>
   <SourceLine>newH = ph / r0</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>q = New Picture(newW, newH)</SourceLine>
   <SourceLine>q.Graphics.DrawPicture(p, 0, 0, newW, newH, 0, 0, pw, ph)</SourceLine>
   <SourceLine>Return q</SourceLine>
   <SourceLine>End Function</SourceLine>
